{
  "name": "docfinder",
  "description": "DocFinder",
  "version": "0.0.1",
  "publisher": "vscode-samples",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "docfinder-sidebar-view",
          "title": "DocFinder",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "docfinder-sidebar-view": [
        {
          "type": "webview",
          "id": "docfinder.colorsView",
          "name": "DocFinder"
        }
      ]
    },
    "commands": [
      {
        "command": "docfinder.addColor",
        "category": "DocFinder",
        "title": "Add Color"
      },
      {
        "command": "docfinder.clearColors",
        "category": "DocFinder",
        "title": "Clear Colors",
        "icon": "$(clear-all)"
      },
      {
        "command": "docfinder.findDocs",
        "category": "DocFinder",
        "title": "Search For Docs",
        "when": "editorHasSelection"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "docfinder.clearColors",
          "group": "navigation",
          "when": "view == docfinder.colorsView"
        }
      ],
      "editor/context": [
        {
          "command": "docfinder.findDocs",
          "group": "1_modification",
          "when": "editorTextFocus"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "typescript": "^4.8.4"
  }
}
